  "yhintConfig": {
    "json": true
  },
  "jshintConfig": {
    "esnext": true,
    "white": true,
    "onevar": true,
    "node": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "immed": true,
    "latedef": true,
    "newcap": true,
    "nonew": true,
    "plusplus": false,
    "quotmark": "single",
    "undef": true,
    "unused": true,
    "trailing": true,
    "maxparams": 5,
    "maxcomplexity": 8,
    "globals": {
      "$": true,
      "$$": true,
      "afterEach": true,
      "assert": true,
      "before": true,
      "beforeEach": true,
      "browser": true,
      "describe": true,
      "expect": true,
      "it": true,
      "xdescribe": true,
      "xit": true,
      "window": true,
      "document": true,
      "vis": true,
